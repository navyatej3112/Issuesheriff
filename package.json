{
  "name": "issuesheriff",
  "version": "1.0.0",
  "description": "GitHub Action to auto-triage Issues and PRs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "npm run build && ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "watch": "tsc --watch"
  },
  "keywords": [
    "github-action",
    "triage",
    "issues",
    "pull-requests"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.3.3"
  }
}

